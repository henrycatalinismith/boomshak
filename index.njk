---
title: boomshak
description: vector font
---
<!doctype html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8" />

    <link
      rel="canonical"
      href="https://hen.cat/boomshak"
    />

    <meta
      name="viewport"
      content="width=device-width, initial-scale=1"
    />

    <title>
      {{ title }}
    </title>

    <style>
      {{ css | safe }}
    </style>
  </head>
  <body>
    <main>

      <header>
        <h1>
          <span>
            title
          </span>
          {% image_boomshak %}
        </h1>
        <p>
          {{ package.description }}
        </p>
      </header>

      {% image_characters %}

      {{ readme.abstract | safe }}

      <aside>
        {% image_animation %}
      </aside>

    </main>
  </body>
</html>
